
rule all:
    input:
        "data/interim/after_normilize.csv",
        "data/interim/with_dummy.csv"

rule add_features:
    input:
        "data/interim/after_normilize.csv"
    output:
        "data/interim/with_dummy.csv"
    shell:
        "python -m src.add_features {input} {output}"

rule normilize:
    input:
        "data/raw/train.csv"
    output:
        "data/interim/after_normilize.csv"
    shell:
        "python -m src.normalize {input} {output}"